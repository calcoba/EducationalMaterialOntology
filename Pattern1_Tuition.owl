###  http://www.is-unit4.com/pattern/tuition.owl#studiesSubject
:studiesSubject rdf:type owl:AnnotationProperty ;
                rdfs:subPropertyOf rdf:value ;
                rdfs:range :Subject .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#value
rdf:value rdf:type owl:AnnotationProperty .


#################################################################
